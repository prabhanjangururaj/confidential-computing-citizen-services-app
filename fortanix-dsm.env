# Fortanix DSM Configuration for application

# Fortanix DSM Endpoint
FORTANIX_DSM_ENDPOINT=
APP_FORTANIX_DSM_ENDPOINT=

# Your Fortanix DSM API Key (replace with your actual API key)
FORTANIX_API_KEY=

# Your Fortanix DSM Key ID for encryption
FORTANIX_KEY_ID=

# Your Fortanix DSM App ID
FORTANIX_APP_ID=

# Database configuration
DB_PATH=/app/backend/database/citizen_services.db

# Authentication method: 'api_key' or 'trusted_ca'
FORTANIX_AUTH_METHOD=api_key

# Application configuration
NODE_ENV=production
PORT=3001
